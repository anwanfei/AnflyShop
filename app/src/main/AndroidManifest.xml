<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anfly.anflyshop">
    <!-- SD卡缓存图片 -->
    <application
        android:name=".app.AnflyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.sort.CatalogDetailsActivity"></activity>
        <activity android:name=".ui.test.Base64GetValueActivity" />
        <activity android:name=".ui.specialtopic.SpecialTopicActivity" />
        <activity android:name=".ui.webview.WebviewActivity" />
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.Login.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.BrandDetailsActivty" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>